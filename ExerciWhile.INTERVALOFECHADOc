#include <stdio.h>
//#imprimir o intervalo fechado entre dois números
int main (int argc, char *argv[]) {
	
	int n1;
	int n2;
	printf("Digite o número 1: ");
	scanf("%d",&n1);
	printf("\n");
	printf("Digite o número 2: ");
	scanf("%d",&n2);
	printf("\n");

    while (n1 >=n2) {
		printf("%d \n",n1);
		n1 = n1 - 1
		}
	
	while (n1 <= n2) {
		printf("%d \n",n1);
		n1 = n1 + 1;
	}
	return 0;
}
	
	
